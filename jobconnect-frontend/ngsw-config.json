{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/manifest.webmanifest",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/icons/**",
                    "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "api-freshness",
            "urls": [
                "/api/**"
            ],
            "cacheConfig": {
                "strategy": "freshness",
                "maxSize": 100,
                "maxAge": "1h",
                "timeout": "10s"
            }
        },
        {
            "name": "fonts",
            "urls": [
                "https://fonts.googleapis.com/**",
                "https://fonts.gstatic.com/**"
            ],
            "cacheConfig": {
                "strategy": "performance",
                "maxSize": 20,
                "maxAge": "365d"
            }
        }
    ]
}